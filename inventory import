awx login --conf.host https://192.168.253.137/ --conf.username admin --conf.password 'redhat123' -k

awx inventory create \
  --name "VGS-CLUSTER" \
  --organization "Default" \
  --conf.host https://192.168.253.137/ \
  --conf.token "y7g8YtjpFGM95gGQbTFUds84NFoXo9" \
  -k


docker cp /root/hosts.ini awx_task:/tmp/hosts.ini
docker exec -it awx_task bash
awx-manage inventory_import \
  --inventory-name "VGS-CLUSTER" \
  --source "/tmp/hosts.ini" \
  --overwrite


vi /root/hosts.ini

